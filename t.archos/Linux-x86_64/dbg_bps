for a in . .. ../.. ; do [ -e $a/tests.sh ] && . $a/tests.sh ; done

# "!" - run commands (via system)

NAME='dbg.dcC+ds.swstep1'
FILE=../../bins/elf/analysis/tiny-crackme-vm-x86_64
ARGS=-d
BROKEN=
CMDS='e scr.color=0
e dbg.swstep=true
ds 0x004000b6
dc
dr PC
'
EXPECT='
'
run_test
