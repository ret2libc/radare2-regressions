#!/bin/sh

for a in . .. ../.. ; do [ -e $a/tests.sh ] && . $a/tests.sh ; done

NAME="sega megadrive rom info"
FILE=../../bins/smd/LiquidSpaceDodgerV3.bin
ARGS=''
BROKEN=
CMDS="i~!fd"
EXPECT='blksz    0x0
block    0x100
file     ../../bins/smd/LiquidSpaceDodgerV3.bin
format   smd
iorw     false
mode     r-x
size     0x272f8
humansz  156.7K
type     ROM
arch     m68k
binsz    160504
bits     16
canary   false
class    SEGA MEGASIS    (C)2015 James L.
crypto   false
endian   little
havecode true
linenum  false
lsyms    false
machine  Sega Megadrive
nx       false
os       smd
pic      false
relocs   false
static   true
stripped false
va       true
'
run_test

NAME="sega megadrive rom symbols"
FILE=../../bins/smd/LiquidSpaceDodgerV3.bin
ARGS=''
CMDS="isq"
EXPECT='0x00000000 0 rom_start
0xf8720200 0 rom_end
0x0000ff00 0 ram_start
0xffffff00 0 ram_end
0x00fffff0 0 SSP
0x00000200 0 Reset
0x00001300 0 BusErr
0x0000130c 0 InvOpCode
0x000012a0 0 DivBy0
0x000012ac 0 Check
0x000012b8 0 TrapV
0x000012c4 0 GPF
0x000012d0 0 Trace
0x000012dc 0 Reserv0
0x000012e8 0 Reserv1
0x000012f4 0 Reserv2
0x00001294 0 Reserv3
0x444f4e27 0 Reserv4
0x54204c4f 0 BadInt
0x4f4b2041 0 Reserv10
0x54205448 0 Reserv11
0x49532100 0 Reserv12
0x00001294 0 Reserv13
0x00001294 0 Reserv14
0x00001294 0 Reserv15
0x00001294 0 Reserv16
