#!/bin/sh
for a in . .. ../.. ; do [ -e $a/tests.sh ] && . $a/tests.sh && break ; done

NAME='x86 - init_array detection'
FILE=../../bins/elf/x86-helloworld-gcc
CMDS='S~init_array
'
EXPECT='[17] . 0x0000059c rw- va=0x0804959c sz=0x0004 vsz=0x0004 .init_array
'
run_test

