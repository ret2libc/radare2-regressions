for a in . .. ../.. ; do [ -e $a/tests.sh ] && . $a/tests.sh ; done

NAME='z xref'
FILE=../bins/elf/hello-linux-x86_64
CMDS='
s entry0
zb test fc044000e8b7
fs sign
f~?
.z/ $$ $$+1024
f~?
'
EXPECT='0
1
'
run_test

NAME='zb'
FILE=../bins/elf/go_stripped
CMDS='
zb sym.fmt.Println 653b2530000000724b53b834e7150883ec188b15401c22088d4c24048b5c242083ec08ff742434ff742434ff742434525051e829ffffff8b44242089038b4424248943048b44242889430889d883c4345bc204006a106a1ce85e350700c20400
z
'
EXPECT='Loaded 1 signatures
  1 byte signatures
  0 head signatures
  0 func signatures
Found 0 matches
'
run_test

NAME='z/'
FILE=../bins/elf/go_stripped
CMDS='
zb sym.fmt.Println 653b2530000000724b53b834e7150883ec188b15401c22088d4c24048b5c242083ec08ff742434ff742434ff742434525051e829ffffff8b44242089038b4424248943048b44242889430889d883c4345bc204006a106a1ce85e350700c20400
.z/ section..text section..text+1074039
?v sign.sys.b.sym.fmt.Println
'
EXPECT='0x805b030
'
run_test


NAME='z + aac + axt'
FILE=../bins/elf/go_stripped
CMDS='
zb sym.fmt.Println 653b2530000000724b53b834e7150883ec188b15401c22088d4c24048b5c242083ec08ff742434ff742434ff742434525051e829ffffff8b44242089038b4424248943048b44242889430889d883c4345bc204006a106a1ce85e350700c20400
.z/ section..text section..text+1074039
e anal.hasnext=0
aac
axt 0x805b030
'
EXPECT='call 0x8049adb call sign.sys.b.sym.fmt.Println in fcn.08049a6c
'
run_test

NAME='zg on mach0'
FILE=../bins/mach0/mach0-i386
BROKEN=1
CMDS='
aaa
zg i386
'
EXPECT='zn i386
zb sym.__mh_execute_header 00015f000500045f6d685f657865637574655f686561646572002b626172002f666f6f00346d61696e0039020000000300801e000300d01e000300801f000000801e503000000000fade0c0500000014000000010000000000000000020000000f01100000100000160000000f010000001f00001b0000000f010000501f0000
zb sym._bar 5589e583ec0c8b45088945fcc745f800000000c745f4000000008b45f43b45fc0f8d1b0000008b45f48b4df801c1894df88b45f405010000008945f4e9d9ffffff8b45f883c40c5dc3
zb sym._foo 5589e583ec188b45088945fc8b45fc8b4dfc81c103000000890c248945f8e88dffffff8b4df801c189c883c4185dc3
zb main 5589e583ec188b450c8b4d08c745fc00000000894df88945f48b45f8890424e8acffffff83c4185dc3
zn-
'
run_test
