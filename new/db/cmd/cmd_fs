NAME=md iso
FILE=../bins/fs/iso.img
EXPECT=<<EOF
d .
d ..
f LS.
f README.MD
f SERVICES.
EOF
CMDS=<<EOF
md /root
EOF
RUN

NAME=md iso3
FILE=../bins/fs/iso3.img
EXPECT=<<EOF
d .
d ..
d bin
d etc
f README.md
EOF
CMDS=<<EOF
md /root
EOF
RUN

NAME=mp gpt
FILE=../bins/fs/gpt.img
EXPECT=<<EOF
0 00 0x0000100000 0x0020100000
1 00 0x0020100000 0x1b24700000
2 00 0x1b24700000 0x1d1eb00000
EOF
CMDS=<<EOF
mp gpt
EOF
RUN

NAME=m iso9660 auto
FILE=../bins/fs/iso.img
EXPECT=<<EOF
iso9660	0x0	/root
EOF
CMDS=<<EOF
m
EOF
RUN

NAME=m ext2 auto
FILE=../bins/fs/ext2.img
EXPECT=<<EOF
ext2	0x0	/root
EOF
CMDS=<<EOF
m
EOF
RUN

NAME=m hfs auto
FILE=../bins/fs/hfs.img
EXPECT=<<EOF
hfs	0x0	/root
EOF
CMDS=<<EOF
m
EOF
RUN

NAME=m fat auto
FILE=../bins/fs/fat.img
EXPECT=<<EOF
fat	0x0	/root
EOF
CMDS=<<EOF
m
EOF
RUN

NAME=md ext2 auto
FILE=../bins/fs/ext2.img
EXPECT=<<EOF
6
EOF
ARGS=-n
CMDS=<<EOF
m /root
md /root~?
EOF
RUN

NAME=md ext2
FILE=../bins/fs/ext2.img
EXPECT=<<EOF
d .
d ..
d lost+found
d bin
d etc
f README.md
EOF
ARGS=-n
CMDS=<<EOF
m /root ext2
md /root
EOF
RUN

NAME=md ext2 cat file
FILE=../bins/fs/ext2.img
EXPECT=<<EOF
This is an EXT2 partition

EOF
ARGS=-n
CMDS=<<EOF
m /root ext2
mc /root/README.md
EOF
RUN

NAME=md vfat
FILE=../bins/fs/fat.img
EXPECT=<<EOF
d etc
d bin
f README.md
EOF
ARGS=-n
CMDS=<<EOF
m /root fat
md /root
EOF
RUN

NAME=md fat cat file
FILE=../bins/fs/fat.img
EXPECT=<<EOF
This is a FAT partition

EOF
ARGS=-n
CMDS=<<EOF
m /root fat
mc /root/README.md
EOF
RUN

NAME=md hfs
FILE=../bins/fs/hfs.img
EXPECT=<<EOF
d bin
d etc
f README.md
EOF
ARGS=-n
CMDS=<<EOF
m /root hfs
md /root
EOF
RUN

NAME=md hfs cat file
FILE=../bins/fs/hfs.img
EXPECT=<<EOF
This is an HFS partition

EOF
ARGS=-n
CMDS=<<EOF
m /root hfs
mc /root/README.md
EOF
RUN
